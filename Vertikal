int main()
{
    char matrix[3][3]{
        {'B','A','T'},
        {'U','R','A'},
        {'T','A','O'}
    };
    char matrix_2[5][5]{// 01,12,23
        {'B','A','T','A','K'},
        {'U','R','A','M','E'},
        {'T','A','O','E','R'},
        {'A','B','K','R','A'},
        {'H','A','A','I','M'}
    };
    
    string kata;
    cin >> kata;
    
    transform(kata.begin(),kata.end(),kata.begin(), ::toupper);
    
    cout << pencaian(kata,matrix_2);
    
    return 0;
}

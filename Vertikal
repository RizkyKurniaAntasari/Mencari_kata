    int aturan = 1;
while(jalan){
    for(int i = start; i != end; i+=k){
        for(int j = start; j != end; j+=k){

            if(kata[index] == matrix_2[j][i]){
                index++;
                ketemu+=matrix_2[j][i];
            }
            
              //debug
             cout << "HURUF : " << matrix_2[i][j] << " POSISI j - i = " << j << "-" << i << " ";
             cout << " ,ketemu : " << ketemu << endl;
 
            
            if(ketemu == kata){
                return true;
            }
            
        }
        ketemu = "";
        index = 0;
    }
    aturan++;
    if(aturan == 3){
        jalan = false;
        break;
    }
    swap(start,end);
    k = -1;
    cout << "Next Iter : started ! " << endl; 
} // while


